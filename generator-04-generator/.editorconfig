# EditorConfig is awesome: http://EditorConfig.org
root = true

[*]
charset = utf-8
indent_style = tab
indent_size = 1
end_of_line = lf
insert_final_newline = true
trim_trailing_whitespace = true
max_line_length = 120

[*.js]
quote_type = double
curly_bracket_next_line = true
spaces_around_brackets = outside

